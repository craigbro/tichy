	function isWin(){ if (navigator.appVersion.indexOf("Win") != -1) { return true; } else { return false; } }
	if (isWin() && bName != "Netscape")
		document.write("<input name=\"query\" class=\"FormLt1\" type=text maxlength=\"40\" size=\"35\">");
